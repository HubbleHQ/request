{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
      "include": ["./src/"],
      "exclude": ["node_modules"],
      "includePattern": ".js$"
    },
    "sourceType": "module",
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    },
    "templates": {
      "referenceTitle": "@hubblehq/request"
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./docs/",
      "recurse": true,
      "template": "./node_modules/jsdoc-template",
      "readme": "./README.md"
    }
  }
  